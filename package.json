{
  "name": "@protonapp/barchart-component",
  "version": "0.0.5",
  "description": "Chart Components Component for Foundry",
  "main": "index.js",
  "scripts": {
    "build": "proton build",
    "publish": "S3_BUCKET=proton-libraries S3_REGION=us-east-1 proton publish",
    "start": "FOUNDRY_DEV_SERVER=http://localhost:3035 npx foundry dev",
    "prettier": "prettier --write *.js src/*.js \"src/**/*.js\""
  },
  "author": "Kedar Bell",
  "license": "ISC",
  "proton": {
    "displayName": "Charts",
    "components": [
      {
        "name": "BarChart",
        "manifest": "./src/BarChart/BarChartManifest.json"
      },
      {
        "name": "LineChart",
        "manifest": "./src/LineChart/LineChartManifest.json"
      }
    ]
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": "*",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@protonapp/proton-cli": "^0.0.23",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "react": "^16.5.2",
    "react-art": "^16.6.0",
    "react-dom": "^16.5.2",
    "react-native-web": "^0.9.5",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "react-native-svg": "9.13.3",
    "victory-native": "^32.0.2"
  }
}
