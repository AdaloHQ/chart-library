{
  "displayName": "Bar Chart",
  "icon": "./icon.png",
  "defaultWidth": 300,
  "defaultHeight": 300,
  "component": "./BarChart.js",
  "resizeY":true,
  "props": [
    {
      "name": "barchartdesc",
      "displayName": "What is this a chart of?",
      "type": "list"
    },
    {
      "name":"yaxis",
      "displayName": "Y Axis Value",
      "type":"number",
      "role": "listItem",
      "reference":"barchartdesc"
    },
    {
      "name":"xaxis",
      "displayName": "X Axis Value",
      "type":"text",
      "default": "IV",
      "role": "listItem",
      "reference":"barchartdesc"
    },
    {
      "name":"baraction",
      "displayName":"Bar Action",
      "type": "action",
      "role": "listItem",
      "reference":"barchartdesc"
    }
  ],"childComponents": [
    {
      "name": "barchartstyles",
      "displayName": "Styles",
      "props": [
        {
          "name": "chart_title",
          "displayName": "Chart Title",
          "type": "text",
          "default": ""
        },
        {
          "name": "chart_subtitle",
          "displayName": "Chart Subtitle",
          "type": "text",
          "default": ""
        },
        {
          "name":"y_axis_label",
          "displayName": "Y Axis Label",
          "type":"text",
          "default":""
        },
        {
          "name": "x_axis_label",
          "displayName": "X Axis Label",
          "type":"text",
          "default":""
        },
        {
          "name": "bar_color",
          "displayName": "Bar Color",
          "type": "color",
          "default": "@primary"
        },
        {
          "name": "toggle_label",
          "displayName": "Toggle Y Labels",
          "type":"boolean"
        }
      ]
    }
  
  ]
}

